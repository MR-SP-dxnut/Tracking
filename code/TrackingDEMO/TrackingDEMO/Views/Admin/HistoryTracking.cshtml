@{
    Layout = "_Layout_Adminn";
    

    var i = 1 ;
}
<body class="body-white">
    <div class="main">
        <br/>
        <br/>
        <div class="container">
              <br/>
                <table class="table">
                    <thead>
                        <tr class="text-center">
                            <th class="col-2">ลำดับ</th>
                            <th class="col-3">Tracking Number</th>
                            <th class="col-2">ชื่อสินค้า</th>
                            <th class="col-5">ดูรายละเอียด</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody> 
                        @foreach (var item in @ViewBag.showTable)
                       {
                           <tr class="text-center ">
                            <td >
                                <div>
                                    <br/>
                                    @i 
                                    <br/>
                                    <br/> 
                                </div>
                            </td>
                            <td >
                                <div >
                                    <br/>
                                    @item.id 
                                    <br/>
                                    <br/> 
                                </div>
                            </td>
                            <td >
                                <div>
                                    <br/>
                                    @item.name_Product 
                                    <br/>
                                    <br/> 
                                </div>
                            </td>
                            <td >
                                <div >
                                    <br/>
                                    @item.name_Status 
                                    <br/>
                                    <br/> 
                                </div>
                            </td>
                            </tr> 
                            
                            i++;
                       }
                    </tbody>
                </table>
        </div>
        <br/>
    </div>
</body>
